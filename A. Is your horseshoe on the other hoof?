#include <bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    cout.tie(nullptr);
    long long c[4];
    int ans = 0;
    for (int i = 0; i < 4; i++)
    { 
        cin >> c[i];
        if(count(c,c+4,c[i]) > 1)
            ans++;
    }
    cout<<ans<<endl;
    return 0;
}
